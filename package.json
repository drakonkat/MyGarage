{
  "name": "promemoria-manutenzione-auto",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "npm run build && npm run start:backend",
    "dev:backend": "npm run dev --prefix backend",
    "start:backend": "npm start --prefix backend",
    "setup:backend": "npm install --prefix backend && npm run db:setup --prefix backend",
    "dev:full": "concurrently \"npm:dev\" \"npm:dev:backend\""
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "mobx-react-lite": "^4.1.0",
    "@mui/material": "^7.3.2",
    "@mui/icons-material": "^7.3.4",
    "@mui/x-data-grid": "^7.11.0",
    "mobx": "^6.13.7",
    "@google/genai": "^1.17.0",
    "idb": "^8.0.0",
    "recharts": "^2.12.7",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.9.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "concurrently": "^8.2.2"
  }
}